{
    "name": "Tensorflow Project",
    "dockerComposeFile": [
        "../docker-compose.yaml",
        "docker-compose.yml"
    ],
    "service": "tensorflow",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
				"ms-python.vscode-pylance"
            ]
        }
    }
    // ... rest of your configuration ...
}


